{
  "id": "webtoons",
  "name": "Webtoons",
  "version": "1.0.0",
  "description": "Download webtoons from webtoons.com",
  "author": "Mango-Go Community",
  "license": "MIT",
  "api_version": "1.0",
  "plugin_type": "downloader",
  "entry_point": "index.js",
  "capabilities": {
    "search": true,
    "chapters": true,
    "download": true
  },
  "config": {
    "base_url": {
      "type": "string",
      "default": "https://www.webtoons.com",
      "description": "Webtoons base URL"
    },
    "mobile_url": {
      "type": "string",
      "default": "https://m.webtoons.com",
      "description": "Webtoons mobile URL"
    }
  }
}

